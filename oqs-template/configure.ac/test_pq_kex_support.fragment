{%- for kem in config['kems'] %}
	AC_MSG_CHECKING([for libOQS {{ kem['family'] }} support])
	AC_COMPILE_IFELSE([AC_LANG_PROGRAM([[
				#include <oqs/oqs.h>
			]], [[
					#if defined(ENABLE_KEM_{{ kem['family'] }}) || ({%- for variant in kem['variants'] -%}defined({{ variant['oqs_meth']|replace("KEM_alg","ENABLE_KEM") }}){% if not loop.last -%}{{ " && " }}{%- endif -%}{%- endfor -%})
					#else
					#error "{{ kem['family'] }} is not supported"
					#endif
			]])],
			[ AC_MSG_RESULT([yes])
				AC_DEFINE([HAVE_{{ kem['family'] }}], [1], [Define to 1 if LibOQS library is compiled with support for {{ kem['family'] }}])
			], [AC_MSG_RESULT([no])
	])
{% endfor %}
